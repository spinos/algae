setAttr ( $node + ".minFrame") `intFieldGrp -q -v1 importanemoneRange`;
setAttr ( $node + ".maxFrame") `intFieldGrp -q -v2 importanemoneRange`;
setAttr ( $node + ".frameStep") `intFieldGrp -q -v1 importanemoneStep`;
}

