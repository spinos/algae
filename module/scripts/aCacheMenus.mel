global proc anm_makeObjBkm()
{
	string $path = `workspace -q -fn`+"/rmantmp/";
	string $scn = `file -q -sceneName`;
	string $buffer[];
	$numTokens = `tokenize $scn "/" $buffer`;
	$scn = $buffer[$numTokens-1];
	$numTokens = `tokenize $scn "." $buffer`;
	$scn = $buffer[0];
	$path = $path + $scn;
	
	string $clouds[] = `getFileList -folder $path -filespec "*.ptc"`;

	string $acloud;
	string $cmd;
	string $bkm;
	int $i;
	for($acloud in $clouds) {
		$bkm = "";
		$numTokens = `tokenize $acloud "." $buffer`;
		if($buffer[0] != "global") {
			for($i =0; $i < $numTokens-1; $i++) $bkm = $bkm + $buffer[$i] + ".";
			$bkm = $bkm + "bkm";
			$cmd = "ptc2brick " + $path + "/" + $acloud + " " + $path + "/" + $bkm;
			print($cmd+"\n");
			$cmd = "system(\""+$cmd+"\")";
			evalDeferred $cmd;
		}
	}
}